{
  "functions": {
    "packages/fika-collect-lambda/src/api/*.ts": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/hello-world",
      "dest": "packages/fika-collect-lambda/src/api/hello-world.ts"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
