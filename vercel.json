{
  "framework": null,
  "outputDirectory": "packages/fika-collect-survey-editor/dist",
  "functions": {
    "api/*.ts": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/v1/hello",
      "dest": "api/hello.ts"
    },
    {
      "src": "/api/v1/presign-upload",
      "dest": "api/presign-upload.ts"
    },
    {
      "src": "/api/v1/submit-survey",
      "dest": "api/submit-survey.ts"
    },
    {
      "src": "/api/v1/surveys",
      "dest": "api/list-surveys.ts"
    }
  ]
}
